server 
{
	listen 8080;
	server_name www.streamagogo.fr;#OPTIONNAL
	error_pages  404 resources/error/404.html;
	client_max_body_size 2000;

	location / 
	{
		allow GET POST DELETE;
		return 404 /resources/error/404.html;
		root /resources/;
		index index.html;
		autoindex on;
		cgi_pass.php ./www/bin/php-cgi;
		upload_store /uploads;
	}
		
}

server 
{
	listen 8081;
	server_name www.streamagogo.fr;#OPTIONNAL
	error_pages  409 resources/error/404.html;
	client_max_body_size 2000;

	location / 
	{
		allow GET POST;
		return 405 /resources/error/405.html;
		root /resources/;
		index index.html;
		autoindex on;
		cgi_pass.php ./www/bin/php-cgi;
		upload_store /uploads;
	}
}